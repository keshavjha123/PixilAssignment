{
    "name": "dockerhub-mcp-server",
    "version": "1.0.0",
    "description": "Production-ready MCP server for comprehensive DockerHub integration",
    "main": "dist/index.js",
    "keywords": [
        "mcp",
        "docker",
        "dockerhub",
        "ai",
        "model-context-protocol"
    ],
    "author": "Keshav Jha <keshavkumariitkgp97@gmail.com>",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/keshavjha123/PixilAssignment.git"
    },
    "bugs": {
        "url": "https://github.com/keshavjha123/PixilAssignment/issues"
    },
    "homepage": "https://github.com/keshavjha123/PixilAssignment#readme",
    "scripts": {
        "build": "tsc",
        "start": "node dist/index.js",
        "serve": "npm run build && npm start",
        "dev": "ts-node-dev --respawn --transpile-only src/index.ts",
        "start:dev": "npm run dev",
        "lint": "eslint . --ext .ts --fix",
        "lint:check": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\" \"tests/**/*.ts\"",
        "format:check": "prettier --check \"src/**/*.ts\" \"tests/**/*.ts\"",
        "test": "jest",
        "test:watch": "jest --watch",
        "test:coverage": "jest --coverage",
        "test:private": "node local_test/mcp-bulk-client-private.mjs",
        "test:public": "node local_test/mcp-bulk-client.mjs",
        "clean": "rm -rf dist",
        "prebuild": "npm run clean",
        "prestart": "npm run build",
        "validate": "npm run lint:check && npm run format:check && npm run test",
        "docker:build": "docker build -t dockerhub-mcp-server .",
        "docker:run": "docker run -p 3000:3000 dockerhub-mcp-server"
    },
    "dependencies": {
        "@modelcontextprotocol/sdk": "^1.17.2",
        "axios": "^1.6.0",
        "dotenv": "^16.6.1",
        "node-fetch": "^3.3.2"
    },
    "devDependencies": {
        "@types/jest": "^29.0.0",
        "@types/node": "^20.0.0",
        "@typescript-eslint/eslint-plugin": "^8.39.1",
        "@typescript-eslint/parser": "^8.39.1",
        "eslint": "^8.0.0",
        "jest": "^29.0.0",
        "prettier": "^3.0.0",
        "ts-jest": "^29.0.0",
        "ts-node-dev": "^2.0.0",
        "typescript": "^5.0.0"
    },
    "engines": {
        "node": ">=18.0.0",
        "npm": ">=8.0.0"
    }
}